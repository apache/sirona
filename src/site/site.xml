<project name="Apache Sirona"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/DECORATION/1.0.1"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.1
                             http://maven.apache.org/xsd/decoration-1.0.1.xsd">
  <bannerLeft>
    <name>Apache Sirona</name>
    <src>/images/commons-logo.png</src>
    <alt>Apache Sirona logo</alt>
    <href>/index.html</href>
  </bannerLeft>
  <bannerRight>
    <src>http://incubator.apache.org/images/egg-logo.png</src>
    <href>http://incubator.apache.org/</href>
  </bannerRight>

  <custom>
    <fluidoSkin>
      <topBarEnabled>true</topBarEnabled>
      <sideBarEnabled>false</sideBarEnabled>
      <topBarIcon />
      <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
      <ohloh>
        <projectId>sirona</projectId>
        <widget>stats</widget>
      </ohloh>
    </fluidoSkin>
  </custom>

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>1.3.0</version>
  </skin>

  <body>

    <head>

      <script type="text/javascript">
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-3211522-14', 'apache.org');
        ga('send', 'pageview');

      </script>

    </head>

    <menu name="User Guide">
      <item name="Concepts" href="/concepts.html"/>
      <item name="Basis" href="/basis.html"/>
      <item name="Configuration" href="/configuration.html"/>
      <item name="Instrumentation" href="/instrumentation.html"/>
      <item name="Reporting" href="/reporting.html"/>
      <item name="Plugins" href="/plugins.html"/>
      <item name="JTA" href="/jta.html"/>
      <item name="JPA" href="/jpa.html"/>
      <item name="JDBC" href="/jdbc.html"/>
      <item name="Web" href="/web.html"/>
      <item name="Graphite" href="/graphite.html"/>
      <item name="Cube" href="/cube.html"/>
      <item name="Collector" href="/collector.html"/>
      <item name="Cassandra" href="/cassandra.html"/>
    </menu>

    <menu ref="reports" inherit="bottom"/>

    <menu name="ASF">
      <item name="How Apache Works" href="http://www.apache.org/foundation/how-it-works.html"/>
      <item name="Foundation" href="http://www.apache.org/foundation/"/>
      <item name="Sponsoring Apache" href="http://www.apache.org/foundation/sponsorship.html"/>
